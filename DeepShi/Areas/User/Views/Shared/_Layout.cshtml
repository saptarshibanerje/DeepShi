                      "lengthMenu": [5, 10, 25, 50, 100],

                  });
              }
             /* window.history.forward();*/

        });
        function noBack() {
            window.history.forward();
        }
        function fnShowAlert() {

            var IsShowMessage = '@(TempData["IsShowMessage"])';
            var IsConfirmMessage = '@(TempData["IsConfirmMessage"])';
            if (IsShowMessage=="Y") {
                if (IsConfirmMessage == "Y") {
                    /*alert("haskjhfjdshfljh")*/
                     Swal.fire({
                        type: '@(TempData["MessageType"])',
                        title: '@(TempData["MessageTitle"])',
                         html: '@Html.Raw(TempData["MessageBody"])',
                         allowOutsideClick: false
                    });
                }
                else {
                    Swal.fire({
                        type: '@(TempData["MessageType"])',
                        title: '@(TempData["MessageTitle"])',
                        html: '@Html.Raw(TempData["MessageBody"])',
                        //timer: 1000,
                        showCancelButton: false,
                        showConfirmButton: true,
                        allowOutsideClick: false
                    }).then(function (dismiss) { if (dismiss === 'timer') { } });
                }
            }
        }
        $.validator.addMethod("ddlmval", function (value, element, params) {
            var minval = params[1];
            if (Helper.getDropdownValue("#" + $(element).attr("id")) < minval) {
                return false;
            }
            return true;
        });
        $.validator.unobtrusive.adapters.add('ddlmval', ['minvalue'], function (options) {
            var element = $(options.form).find('select')[0];

            options.rules['ddlmval'] = [element, parseInt(options.params['minvalue'])];
            options.messages['ddlmval'] = options.message;
        });
    </script>
